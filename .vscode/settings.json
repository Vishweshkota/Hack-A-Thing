{
    "nrf-connect.applications": [
        "${workspaceFolder}"
    ],
    "C_Cpp.default.configurationProvider": "nordic-semiconductor.nrf-connect",
    "C_Cpp.intelliSenseEngine": "default",
    "files.associations": {
        "*.h": "c",
        "*.c": "c",
        "stdint.h": "c",
        "kernel.h": "c"
    },
    "C_Cpp.errorSquiggles": "enabled"
}